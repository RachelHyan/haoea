{
	"extends": [
		"stylelint-config-standard",
		"stylelint-config-recess-order"
	],
	"overrides": [
		{
			"files": [
				"**/*.less"
			],
			"customSyntax": "postcss-less"
		},
		{
			"files": [
				"**/*.css"
			],
			"customSyntax": "postcss"
		}
	],
	"rules": {
		"selector-class-pattern": null
	}
}
